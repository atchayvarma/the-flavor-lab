    {
      "cuisine": "Indian",
      "recipe_name": "Dal Makhani",
      "duration": "1 hour 15 minutes (including soaking time)",
      "cost_per_serving": "$1.50",
      "ingredients": [
        {
          "name": "Black Lentils",
          "quantity": "1 cup",
          "walmart_price": "$1.00",
          "amazon_grocery_price": "$1.20"
        },
        {
          "name": "Kidney Beans",
          "quantity": "1 cup",
          "walmart_price": "$0.80",
          "amazon_grocery_price": "$1.00"
        },
        {
          "name": "Butter",
          "quantity": "2 tbsp",
          "walmart_price": "$0.50",
          "amazon_grocery_price": "$0.60"
        },
        {
          "name": "Onion",
          "quantity": "1 medium",
          "walmart_price": "$0.30",
          "amazon_grocery_price": "$0.40"
        },
        {
          "name": "Garlic",
          "quantity": "2 cloves",
          "walmart_price": "$0.10",
          "amazon_grocery_price": "$0.15"
        },
        {
          "name": "Ginger",
          "quantity": "1 inch",
          "walmart_price": "$0.20",
          "amazon_grocery_price": "$0.25"
        },
        {
          "name": "Ripe Tomatoes",
          "quantity": "2",
          "walmart_price": "$0.60",
          "amazon_grocery_price": "$0.80"
        },
        {
          "name": "Garam Masala",
          "quantity": "1 tsp",
          "walmart_price": "$0.50",
          "amazon_grocery_price": "$0.60"
        },
        {
          "name": "Heavy Cream",
          "quantity": "½ cup",
          "walmart_price": "$1.00",
          "amazon_grocery_price": "$1.20"
        },
        {
          "name": "Salt and Pepper",
          "quantity": "to taste",
          "walmart_price": "$0.20",
          "amazon_grocery_price": "$0.25"
        }
      ],
      "pre_cooking_steps": [
        {
          "step": "Rinse black lentils and kidney beans.",
          "duration": "5 minutes"
        },
        {
          "step": "Soak lentils and beans in plenty of water for at least 8 hours or overnight.",
          "duration": "8 hours or overnight"
        }
      ],
      "instructions": [
        {
          "step": "Drain soaked lentils and beans.",
          "duration": "5 minutes"
        },
        {
          "step": "Heat butter in a pot over medium heat.",
          "duration": "2 minutes"
        },
        {
          "step": "Sauté onions until translucent.",
          "duration": "5 minutes"
        },
        {
          "step": "Add minced garlic and grated ginger, sauté for another minute.",
          "duration": "1 minute"
        },
        {
          "step": "Add chopped tomatoes and cook until soft and mushy.",
          "duration": "10 minutes"
        },
        {
          "step": "Add garam masala, lentils, beans, and 4 cups of water to the pot.",
          "duration": "5 minutes"
        },
        {
          "step": "Bring to a boil, then reduce heat and simmer for 45 minutes, or until lentils and beans are tender.",
          "duration": "45 minutes"
        },
        {
          "step": "Stir in heavy cream and season with salt and pepper.",
          "duration": "5 minutes"
        },
        {
          "step": "Simmer for 5 minutes, or until the dal thickens slightly.",
          "duration": "5 minutes"
        }
      ],
